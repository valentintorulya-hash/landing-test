{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/"
      ],
      "numberOfRuns": 1,
      "startServerCommand": "npm run build && npm run start -- -p 3000",
      "startServerReadyPattern": "Ready",
      "startServerReadyTimeout": 180000
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.95
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 0.95
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".sisyphus/evidence/lhci"
    }
  }
}
